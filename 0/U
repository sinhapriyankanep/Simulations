FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    inlet_r
    {
        type            fixedValue;
        value           uniform (-1.8311 0 1.896);
    }
    inlet_l
    {
        type            fixedValue;
        value           uniform (1.8311 0 1.896);
    }
}


