FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
   inlet_r
    {
        type            fixedValue;
        value           uniform 1;
    }
inlet_l
    {
       type            fixedValue;
        value          uniform 1;
    }

   
   outlet
    {
        type                  inletOutlet;
        inletValue      uniform 0;
        value                uniform 0;
    }

    
}

