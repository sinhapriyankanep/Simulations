FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet_r
    {
        type        zeroGradient;
    }
  inlet_l
    {
        type           zeroGradient;
       
    }


    outlet
    {
        type            totalPressure;
        p0                uniform 0;
      //  U                   U;//
        phi               phi;
        rho             rho;
        psi               none;
        gamma           1;
        value           uniform 0;
    }

    
}

